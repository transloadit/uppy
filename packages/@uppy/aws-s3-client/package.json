{
  "name": "@uppy/aws-s3-client",
  "version": "0.0.1",
  "description": "Lightweight S3 client for uppy",
  "bugs": {
    "url": "https://github.com/transloadit/uppy/issues"
  },
  "repository": {},
  "publishConfig": {
    "registry": "https://github.com/transloadit/uppy"
  },
  "files": [
    "./lib/",
    "./src/",
    "./package.json",
    "./README.md"
  ],
  "type": "module",
  "scripts": {
    "test:e2e": "vitest run",
    "typecheck": "tsc --build",
    "build": "tsc --build tsconfig.build.json"
  },
  "main": "lib/s3mini.min.js",
  "module": "lib/s3mini.js",
  "types": "lib/s3mini.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/s3mini.d.ts",
        "development": "./lib/s3mini.js",
        "default": "./lib/s3mini.min.js"
      },
      "require": {
        "types": "./lib/s3mini.d.ts",
        "default": "./lib/s3mini.min.js"
      }
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true,
  "license": "MIT",
  "devDependencies": {
    "dotenv": "17.2.3",
    "typescript": "5.9.3",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "glob": "12.0.0"
  }
}
